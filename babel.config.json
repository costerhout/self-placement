{
  "presets": [
    ["@vue/app", {
      "modules": "cjs",
      "polyfills": [
        "es.array.iterator",
        "es.array.for-each",
        "es.array.from",
        "es.object.assign",
        "es.object.entries",
        "es.promise",
        "es.promise.finally",
        "es.symbol"
      ]
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-shorthand-properties",
    "@babel/plugin-transform-classes",
    "@babel/plugin-transform-arrow-functions"
  ]
}
