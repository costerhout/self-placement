{
  "name": "tidalechoes",
  "version": "0.1.0",
  "description": "Self Placement page",
  "author": "Colin Osterhout",
  "license": "MIT",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "dev": "vue-cli-service serve --port 8090",
    "gen": "gulp xslt && bin/generate.sh",
    "build-fallback": "vue-cli-service build --mode fallback",
    "build-staging": "vue-cli-service build --mode staging",
    "upload": "gulp publish:dist",
    "upload-awsonly": "gulp aws:dist",
    "upload-staging-awsonly": "gulp aws:staging",
    "upload-fallback": "gulp publish:fallback",
    "upload-staging": "gulp publish:staging",
    "inspect": "vue-cli-service inspect | less",
    "inspect-build": "vue-cli-service inspect --mode production | less",
    "inspect-staging": "vue-cli-service inspect --mode staging | less",
    "inspect-fallback": "vue-cli-service inspect --mode fallback | less",
    "clean": "gulp clean",
    "xslt": "gulp xslt",
    "test": "vue-cli-service test:unit --require tests/setup.js",
    "test-debug": "vue-cli-service test:unit --require tests/setup.js --inspect-brk",
    "lint": "vue-cli-service lint"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "overrides": [
      {
        "env": {
          "jest": true
        },
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ]
      }
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "root": true,
    "rules": {}
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^.*\\.(png|svg|jpg|jpeg)(\\?inline)?$": "<rootDir>/../../lib/relative-image-stub.js",
      "^@/(.*)$": "<rootDir>/../../$1"
    },
    "testPathIgnorePatterns": [
      "scratch"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@gravitano/vue2-google-maps|vue-awesome)/)"
    ]
  },
  "vuePlugins": {
    "service": [
      "webpack-configuration-plugin.js"
    ]
  },
  "dependencies": {
    "ernie": "git+https://github.alaska.edu/ctosterhout/ernie#v1.2.1",
    "ernie-core": "git+https://github.alaska.edu/ctosterhout/ernie-core#v0.1.3",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "^4.1.2",
    "@vue/cli-service": "^4.1.0",
    "@vue/test-utils": "^1.0.0-beta.31",
    "autoprefixer": "^9.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "ernie-build": "git+https://github.alaska.edu/ctosterhout/ernie-build#v0.1.4",
    "ernie-theme": "git+https://github.alaska.edu/ctosterhout/ernie-theme#v0.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-header": "^2.0.9",
    "gulp-postcss": "^8.0.0",
    "jest": "^25.1.0",
    "lodash": "^4.17.11",
    "mutationobserver-shim": "^0.3.3",
    "package-json-merge": "^0.0.1",
    "path": "^0.12.7",
    "sass-loader": "^8.0.2",
    "sinon": "^8.1.1",
    "vue-jest": "^3.0.5",
    "vue-svg-loader": "^0.15.0",
    "xml-js": "^1.6.11"
  }
}
